{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "types": ["expo-router/types"],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
  "**/*.ts",
  "**/*.tsx",
  "**/*.d.ts",
  "types/**/*.d.ts",
  ".expo/types/**/*.ts",
  "expo-env.d.ts",
  "workbox-config.js"
],
  "exclude": ["node_modules", "supabase"]
}
